<div id="registro" class="container">
  <h2><%= image_tag "manzana.jpg", width: 100 %> Zona Ingreso a Socios</h2>
  <div class="container-fluid bg-grey">

     <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
     <div class="col-md-4">
     </div>
     <div class="col-md-4">
       <div class="field">
         <%= f.label :correo_electronico %><br />
         <%= f.email_field :email, autofocus: true, autocomplete: "email", size: 50 %>
       </div>

       <div class="field">
         <%= f.label :ContraseÃ±a %><br />
         <%= f.password_field :password, autocomplete: "current-password", size: 50 %>
       </div>

       <% if devise_mapping.rememberable? %>
         <div class="field">
           <%= f.check_box :remember_me %> Recordarme
         </div>
       <% end %>

       <div class="actions">
         <%= f.submit "Ingresar", class: "btn btn-primary" %>
       </div>
     <% end %>

     <%= render "devise/shared/links" %><br />
     <%= link_to "Regresar", root_path, class: 'btn btn-success' %>
     </div>
</div>
</div>
