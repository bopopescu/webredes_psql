<div class="container">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<div id="socios" class="container-fluid-login">
  <div class="row">
       
      <div class="col-md-6 row-login">
        <%= f.label :'Usuario' %>
        <%= f.email_field :email,
                          autofocus: true,
                          autocomplete: "email",
                          class: "form-control",
                          placeholder: "correo electrónico",  
                          align: 'right' %><br>
        <%= f.label :'Contraseña' %>
        <%= f.password_field :password,
                             autocomplete: "current-password",
                             class: "form-control",
                             placeholder: "contraseña" %><br>
        <% if devise_mapping.rememberable? %>
          <%= f.check_box :remember_me %> Recordarme
        <% end %><br><br> 

        <%= f.submit "Entrar", class: "btn btn-primary" %>   
      </div>
    </div>
  </div>
</div>
<% end %>
</div>

 




