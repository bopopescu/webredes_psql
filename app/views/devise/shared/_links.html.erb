<%- if controller_name != 'sessions' %>
  <%= link_to "Ingresar", new_session_path(resource_name),  class:'btn btn-success'  %>
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Registrar", new_registration_path(resource_name),  class:'btn btn-info' %>
<% end %>


