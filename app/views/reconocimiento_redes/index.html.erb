<div class="container">
<p id="notice"><%= notice %></p>

<h1>Reconocimientos Redes</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reconocimiento_redes.each do |reconocimiento_rede| %>
      <tr>
        <td><%= reconocimiento_rede.nombre %></td>
        <td><%= reconocimiento_rede.descripcion %></td>
        <td><%= link_to 'Mostrar', reconocimiento_rede %></td>
        <td><%= link_to 'Modificar', edit_reconocimiento_rede_path(reconocimiento_rede) %></td>
        <td><%= link_to 'Editar', reconocimiento_rede, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Agregar Reconocimiento', new_reconocimiento_rede_path %>
</div>
