<div class="container-fluid-crud">
<p id="notice"><%= notice %></p>

<h1>Reconocimientos Redes</h1>
<%= link_to 'Agregar Reconocimiento', new_reconocimiento_rede_path, class: "btn btn-default btn-lg" if logged_in?(:site_admin) %>

<table calss="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Descripcion</th>
      <th colspan="3">Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @reconocimiento_redes.each do |reconocimiento_rede| %>
      <tr>
        <td><%= reconocimiento_rede.nombre %></td>
        <td><%= reconocimiento_rede.descripcion %></td>
        <td><%= link_to 'Mostrar|', reconocimiento_rede %></td>
        <td><%= link_to 'Modificar|', edit_reconocimiento_rede_path(reconocimiento_rede) %></td>
        <td><%= link_to 'Eliminar', reconocimiento_rede, method: :delete, data: { confirm: 'Â¿Confirma eliminar Registro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


</div>
