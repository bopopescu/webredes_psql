<div id="about" class="container-fluid-grey">
  <div class="row">
      <h2 class="nosotros-titulo"><strong>nosotros</strong>
      <% if user_signed_in? %>
            <%= link_to 'Editar', edit_acercade_path(@about),class: "btn btn-default btn-lg"  %>
      <% end %>
      </h2>

      <div class="col-md-6">
        <strong>¿qué somos?</strong>
        <p><%= @about.que_somos %></p><br>
        <strong>¿qué hacemos?</strong>
        <p><%= @about.que_hacemos %></p><br>
      </div>
      <div class="col-md-6">
        <strong>¿qué buscamos?</strong>
        <p><%= @about.que_buscamos %></p><br>
        <strong>importancia</strong>
        <p><%= @about.importancia %></p><br>
      </div>
  </div>
  <div class="col-md-6 nosotros-enfoque">
    <strong>nuestro enfoque</strong>
    <p><%= @about.enfoque %></p>
  </div>
</div>















